        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GalleryAK"
        mc:Ignorable="d"
        Title="SlideShowTimer" Height="130.08" Width="292.915">
    <Grid Margin="0,0,0,0">
        <StackPanel Margin="0,10,0,63" Orientation="Horizontal">
            <Label Content="Enter Time in Seconds" Height="26" Margin="0,0" />
            <TextBox x:Name="TimerTextBox" KeyDown="TimerTextBox_KeyDown" Width="68" />
        </StackPanel>
    </Grid>
</Window>
